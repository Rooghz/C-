class Solution{
public:	
	
	
	int isPalindrome(string s)
	{
	    // Your code goes here
	    int start = 0;
	    int end = s.length()-1;
	    
	    while(start < end){
	        if(s[start] != s[end]){
	            return 0;
	        }
	        else{
	            start++;
	            end--;
	        }
	                
	    }
	    return 1;
	}

};


---------

Example 1:

Input: S = "abba"
Output: 1
Explanation: S is a palindrome

Example 2:

Input: S = "abc" 
Output: 0
Explanation: S is not a palindrome
